<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <TextView
            android:id="@+id/hello_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView0001"
            tools:layout_editor_absoluteX="119dp"
            tools:layout_editor_absoluteY="127dp" />

    <ImageView
            android:id="@+id/imageView2"
            android:layout_width="167dp"
            android:layout_height="172dp"
            android:layout_marginTop="120dp"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="204dp"
            android:layout_marginEnd="60dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/barrier3"
            app:layout_constraintTop_toTopOf="parent" />

    <ImageView
            android:id="@+id/imageView"
            android:layout_width="113dp"
            android:layout_height="113dp"
            android:layout_marginStart="88dp"
            android:layout_marginTop="104dp"
            app:layout_constraintStart_toStartOf="@+id/hello_tv"
            app:layout_constraintTop_toBottomOf="@+id/hello_tv"
            app:srcCompat="@mipmap/ic_launcher" />

    <android.support.constraint.Barrier
            android:id="@+id/barrier3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="left"
            app:constraint_referenced_ids="imageView,hello_tv,imageView2"
            tools:layout_editor_absoluteX="411dp" />

    <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            tools:layout_editor_absoluteX="207dp"
            tools:layout_editor_absoluteY="162dp" />
</android.support.constraint.ConstraintLayout>